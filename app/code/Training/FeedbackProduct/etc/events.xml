<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="training_feedback_save_after">
        <observer name="training_feedbackproduct_observer_training_feedback_save_after"
                  instance="Training\FeedbackProduct\Observer\SaveFeedbackProducts"/>
    </event>
    <event name="training_feedback_load_after">
        <observer name="training_feedbackproduct_observer_training_feedback_load_after"
                  instance="Training\FeedbackProduct\Observer\LoadFeedbackProducts" />
    </event>
</config>
